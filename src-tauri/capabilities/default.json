{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capability for aqua",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:allow-read",
    "fs:allow-write",
    "fs:allow-exists",
    "fs:allow-create",
    "fs:allow-read-dir",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-copy-file",
    "core:window:allow-set-title",
    "clipboard-manager:allow-write-text",
    "clipboard-manager:allow-read-text"
  ]
}